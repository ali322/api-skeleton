{
  "name": "api-boilerplate",
  "version": "0.0.1",
  "description": "api boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon --inspect --watch 'src/**/*' -e ts --exec 'node -r ts-node/register' ./src/index.ts",
    "lint": "eslint src --ext .ts",
    "doc": "apidoc -i src -o doc",
    "test": "ava",
    "cov": "rimraf test/coverage test/.nyc_output && nyc --reporter=html npm test"
  },
  "ava": {
    "extensions": ["ts"],
    "require": ["ts-node/register"]
  },
  "keywords": [
    "koa"
  ],
  "author": "alichen",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.9.1",
    "dayjs": "^1.8.30",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^4.0.0",
    "koa-router": "^7.4.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.34",
    "typeorm-naming-strategies": "^1.1.0",
    "uuid": "^8.2.0",
    "validatorjs": "^3.18.1",
    "winston": "^3.3.3",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "2.11.3",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-helmet": "^6.0.4",
    "@types/koa-router": "^7.0.39",
    "@types/koa__cors": "^2.2.3",
    "@types/node": "^11.9.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.0.0",
    "@types/validatorjs": "^3.15.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "apidoc": "^0.17.7",
    "ava": "^3.15.0",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "mocha": "^9.0.2",
    "mochawesome": "^6.2.2",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "qs": "^6.10.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
